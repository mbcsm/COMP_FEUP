class Factorial {
 public int ComputeFac(int num){
 int num_aux ;
 int[] x;
 boolean flag;
 x = new int[2];
 flag = !false;
 if (num < 1)
 num_aux = 1;
 else
    if(!false) {
        num_aux = num * (this.ComputeFac(num-1));
    }
    else
        num_aux = num * x.length;

while (num < 1) {
    num = num + 1;
}

 return num_aux;
 }
